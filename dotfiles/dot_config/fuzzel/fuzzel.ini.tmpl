include=~/.config/fuzzel/themes/{{ .color_scheme }}.ini

[main]
font = monospace:size=12
icons-enabled = false
prompt = ""
dpi-aware = no
use-bold = false
inner-pad = 5
vertical-pad = 10
horizontal-pad = 10

[dmenu]
exit-immediately-if-empty = true

[border]
width = 0
radius = 0

[key-bindings]
delete-line-forward = none
next = Down Control+j
prev = Up Control+k
next-page = Shift+j
prev-page = Shift+k
